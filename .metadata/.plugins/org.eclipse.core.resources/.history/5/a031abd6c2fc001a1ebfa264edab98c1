package com.hsbc.demothreading;

import java.io.IOException;

public class Demo {
	
	public static void main(String[] args) {
		for(int i = 1;i<=10;i++)
		{
			System.out.println(i);
			try
			{
				Thread.sleep(1000);
			}
			catch(InterruptedException e)
			{
				e.printStackTrace();
			}
			
		}
	}

}
